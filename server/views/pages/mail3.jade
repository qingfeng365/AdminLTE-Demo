					.box-footer
						.pull-right
							button.btn.btn-default
								i.fa.fa-reply
								span &nbsp;回复
							button.btn.btn-default
								i.fa.fa-share
								span &nbsp;转发
						button.btn.btn-default
							i.fa.fa-trash-o
							span &nbsp;删除
						button.btn.btn-default
							i.fa.fa-print
							span &nbsp;打印