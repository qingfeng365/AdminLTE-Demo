										span &nbsp;Lumia 950 950XL 产品规格.docx
									span.mailbox-attachment-size &nbsp;1,245 KB
										a.btn.btn-default.btn-xs.pull-right
											i.fa.fa-cloud-download
							li
								span.mailbox-attachment-icon.has-img
									img(src='https://i-1.prod-cdn.webapps.microsoft.com/r/image/view/-/5956136/extraHighRes/4/-/Lumia-950-XL-DSIM-hero-jpg.jpg')
								.mailbox-attachment-info
									a.mailbox-attachment-name(href='#')
